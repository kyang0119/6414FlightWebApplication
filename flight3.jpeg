				<div class="et_pb_code_inner"><style>
.exit-intent-popup {
    z-index: 1000 !important;
    position: fixed;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 1;
    background: rgba(33, 33, 33, 0.8);
    transform: translateY(60%) scale(0);
    transition: transform 0.3s cubic-bezier(0.4, 0.0, 0.2, 1);
}

#cb-ei-r, #cb-ei-c { height: 100% !important; }
  
.eibox {
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
}  
  
.exit-intent-popup.visible {
    transform: translateY(0) scale(1);
}  
  
a.smallblue, a.bigblue {
color: #ffffff!important;
  background: #0078d2 !important;
  background: -webkit-gradient(linear, left top, left bottom, from(#0078d2), to(#0078d2)) !important;
  background: linear-gradient(to bottom, #0078d2 0%, #0078d2 100%) !important;
  border: 1px solid #0078d2 !important;
  font-size: 24px !important;
  font-weight: normal !important;
}

a.small-button, a.big-button {
    display: inline-block;
    text-shadow: 0px 0px 0 #0078d2 !important;
    border-radius: 4px !important;
    line-height: 26px;
}

a.smallblue:hover, a.bigblue:hover {
    color: #ffffff !important;
}
@media only screen and (max-width: 370px) {
  #close-ei1, #close-ei2 { font-size: 22px !important; }
  #ei-a, #ei-b { margin-top: 15vh !important; }
}
</style>
<script>
var lastScrollTop = 0;
var eishown = 0;

function detectDevice() {
   var userAgent = navigator.userAgent || navigator.vendor || window.opera; 

   if (/android/i.test(userAgent)) { 
    return "mobile";
   }

   // iOS detection from: https://stackoverflow.com/a/9039885/177710
   if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
    return "mobile";
   }

   return "desktop";
}
  
let deviceType = detectDevice();
 
const CookieService = {

    setCookie(name, value, days) {
        let expires = '';

        if (days) {
            const date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            expires = '; expires=' + date.toUTCString();
        }

        document.cookie = name + '=' + (value || '')  + expires + ';';
    },

    getCookie(name) {
        const cookies = document.cookie.split(';');

        for (const cookie of cookies) {
            if (cookie.indexOf(name + '=') > -1) {
                return cookie.split('=')[1];
            }
        }

        return null;
    }
}  
  
const mouseEvent = e => {
    const shouldShowExitIntent = 
        !e.toElement && 
        !e.relatedTarget &&
        e.clientY < 10;

    if ( !CookieService.getCookie('exitIntentShown') && shouldShowExitIntent && (window.getComputedStyle(document.getElementById("ei-a")).display != "none" || window.getComputedStyle(document.getElementById("ei-b")).display != "none") ) {
       document.removeEventListener('mouseout', mouseEvent);
       document.querySelector('.exit-intent-popup').classList.add('visible');
        
       // Set the cookie when the popup is shown to the user
       CookieService.setCookie('exitIntentShown', true, 30);
    }
};

const exit = e => {
    const shouldExit =
        [...e.target.classList].includes('exit-intent-popup') || // user clicks on section mask
        [...e.target.classList].includes('exit-intent-popup-col') || // user clicks on column mask
        e.target.className === 'close' || // user clicks on the close icon
        e.keyCode === 27; // user hits escape

    if (shouldExit) {
      if ( window.getComputedStyle(document.getElementById('ei-a')).display!="none" ) {
       closeExitIntent();
      } else {
       closeExitIntent2();
      }
    }
};
  
function closeExitIntent() {
  document.querySelector('.exit-intent-popup').classList.remove('visible');
  eishown = 1;
  // Returns users to the top of page, approx where the HERO is
  var divToLand = document.getElementById('main-content');
  divToLand.scrollIntoView({behavior: "smooth"});
  CookieService.setCookie('exitIntentShown', true, 30);
  return false;
}  
  
function closeExitIntent2() {
  document.querySelector('.exit-intent-popup').classList.remove('visible');
  eishown = 1;
  // Returns users to the top of page, approx where the HERO is
  const id = 'benefits-anchor';
  const yOffset = -112; 
	const element = document.getElementById(id);
	const y = element.getBoundingClientRect().top + window.pageYOffset + yOffset;
  window.scrollTo({top: y, behavior: "smooth"});
  CookieService.setCookie('exitIntentShown', true, 30);
  return false;
} 

document.querySelector('.exit-intent-popup').addEventListener('click', exit);  
  
setTimeout(() => {
    document.addEventListener('mouseout', mouseEvent);
    document.addEventListener('keydown', exit);
}, 4_000);

  // element should be replaced with the actual target element on which you have applied scroll, use window in case of no target element.
document.addEventListener("scroll", function(){ // or window.addEventListener("scroll"....
   var st = window.pageYOffset || document.documentElement.scrollTop; // Credits: "https://github.com/qeremy/so/blob/master/so.dom.js#L426"
  
   console.log('st:'+st+' lst:'+lastScrollTop+' eis:'+eishown+' dT:'+deviceType+' ei-a:'+window.getComputedStyle(document.getElementById("ei-a")).display+' ei-b'+window.getComputedStyle(document.getElementById("ei-b")).display );
  
   if ( !CookieService.getCookie('exitIntentShown') && st < lastScrollTop && eishown == 0 && deviceType == "mobile" && 
      (window.getComputedStyle(document.getElementById("ei-a")).display != "none" || window.getComputedStyle(document.getElementById("ei-b")).display != "none") ){
        // display exit intent
        document.removeEventListener('mouseout', mouseEvent);
        document.querySelector('.exit-intent-popup').classList.add('visible');
        
        // Set the cookie when the popup is shown to the user
        CookieService.setCookie('exitIntentShown', true, 30);
   }
  // if the user scrolled down and the new value is bigger then the old value update lastScrollTop so we only get the lowest point you scrolled to
  if (st > lastScrollTop) { lastScrollTop = st; } 
   
}, false);
  
if (!CookieService.getCookie('exitIntentShown')) {
    setTimeout(() => {
        document.addEventListener('mouseout', mouseEvent);
        document.addEventListener('keydown', exit);
    }, 4_000);
}
  
</script></div>
